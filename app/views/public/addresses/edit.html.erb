<div>
  <h1>配送先編集</h1>
</div>

<div>
  <%= render 'error', address: @address %>
</div>

<div>
   <%= form_with model: @address, method: :patch do |f| %>
    <h4>郵便番号(ハイフンなし)</h4>
      <%= f.text_field :postcode %>
    <h4>住所</h4>
      <%= f.text_field :address %>
    <h4>宛名</h4>
    　<%= f.text_field :name %>
    　<%= f.submit  "変更内容を保存", class: "btn btn-success" %>
  <% end %>
</div>