<div class='container'>
  <div class='row'>
    <div class='col-md-4'>

      <h1>ジャンル編集</h1>

      <%= form_with model: @genre,url:admin_genre_path,local:true do |f| %>

      <div class="form-group">
        <%= f.label :name, 'ジャンル名' %>
        <%= f.text_field :name, class: 'form-control genre_name' %>
      </div>

      <div class="form-group">
        <%= f.submit class: 'btn btn-success' %>
      </div>

      <% end %>

    </div>
  </div>
</div>